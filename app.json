{
  "expo": {
    "scheme":"Clinto",
    "name": "Clinto",
    "slug": "Clinto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": [
            "./assets/sounds/starwar.wav"
          ],
          "mode": "production"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCOSH383cU0Ywb6J1JZA_vlRq6Y6I6DgtE"
      },
      "bundleIdentifier": "in.poa.medportal",
      "requireFullScreen": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCOSH383cU0Ywb6J1JZA_vlRq6Y6I6DgtE"
        }
      },
      "package": "in.poa.medportal"
    },
    "notification": {
      "icon": "./assets/48x48.png",
      "color": "#000000"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
